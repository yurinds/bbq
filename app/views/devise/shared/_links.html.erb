<%- if controller_name != 'sessions' %>
  <nav class="nav">
    <%= link_to "Войти", new_session_path(resource_name), class: "nav-link pl-0" %>
    <%= link_to "Забыли пароль?", new_password_path(resource_name), class: "nav-link pl-0" %>
  </nav>
<% end %>
<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <nav class="nav">
    <%= link_to "Регистрация", new_registration_path(resource_name), class: "nav-link pl-0" %>
  </nav>
<% end %>
