<%= form_with model: [@event, photo], local: true do |f| %>
  <%= render partial: 'partials/form_errors', locals: {model: photo} %>
  <div class="form-group">
    <%= f.label :photo %>
    <%= f.file_field :photo %>
  </div>
  <div class="actions">
    <%= f.submit 'Загрузить', class: 'btn btn-primary' %>
  </div>
<% end %>
